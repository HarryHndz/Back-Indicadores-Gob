  "main": "index.js",
  "scripts": {
    "start": "node dist/index.js",
    "typeorm":"tsx ./node_modules/typeorm/cli.js",
    "migration:generate":"npm run typeorm -- migration:generate -d src/config/db.ts src/migrations/init-migration",
    "migration:run":"npm run typeorm -- migration:run -d src/config/db.ts",
    "migration:revert":"npm run typeorm migration:revert -d src/config/db.ts",
    "dev": "tsx watch src/index.ts",
    "build": "tsc"
  },
  "keywords": [],
