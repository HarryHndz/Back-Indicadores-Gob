  "type": "module",
  "dependencies": {
    "dotenv": "^16.6.1",
    "express": "^4.17.3",
    "express-jwt": "^8.5.1",
    "express-useragent": "^1.0.15",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.18.0",
    "reflect-metadata": "^0.2.2"
  },
  "devDependencies": {
    "@types/express": "4.17",
    "@types/node": "^25.2.3",
    "nodemon": "^3.1.11",
    "tsx": "^4.21.0",
    "typeorm": "^0.3.28",
    "typescript": "^5.9.3"
  }
